{
  "name": "SafeSpace",
  "description": "Making the internet a safe space",
  "version": "1.0",
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js"]
    }
  ],

  "browser_action": {
    "default_title": "Make this page red"
  },
  "manifest_version": 2
}