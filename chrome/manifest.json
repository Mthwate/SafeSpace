{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_title":  "make this is red",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>",
    "storage"
  ],

 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["background.js","popup.js"]
    }
  ]
}
